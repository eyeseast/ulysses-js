{
  "name": "ulysses-js",
  "version": "1.2.1",
  "license": "MIT",
  "main": "dist/ulysses.cjs.js",
  "module": "dist/ulysses.esm.js",
  "browser": "dist/ulysses.umd.js",
  "dependencies": {
    "mapbox-gl": "^1.12.0"
  },
  "devDependencies": {
    "@turf/bbox": "^6.0.1",
    "documentation": "^12.3.0",
    "esm": "^3.2.25",
    "jest": "^26.4.2",
    "rollup": "^1.32.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.1.0",
    "serve": "^11.3.2"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "pretest": "npm run build",
    "prepublish": "npm run build",
    "start": "serve -n docs/",
    "docs": "documentation build -c documentation.yml -f html -o docs src/**",
    "docs:dev": "documentation serve -c documentation.yml -w src/** -w docs/*.md"
  },
  "files": [
    "dist"
  ]
}
